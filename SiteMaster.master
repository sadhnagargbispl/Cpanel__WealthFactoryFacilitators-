<script>
    $(document).ready(function () {
        $(".meanmenu-reveal").click(function () {
            $("nav.mean-nav").toggleClass("show");
        });
    });

    $(document).ready(function () {
        $(".rad-toggle-btn").click(function () {
            $(".rad-logo-container").toggleClass("rad-nav-min");
            $(".rad-sidebar").toggleClass("rad-nav-min");
            $(".rad-body-wrapper").toggleClass("rad-nav-min");
        });

        $(".rad-menu-item").click(function () {
            $(".rad-dropmenu-item").toggleClass("active");
        });

    });

    $(document).ready(function () {
        if ($(window).width() < 769) {
            $('#menu').addClass('rad-nav-min');
        } else {
            $('#menu').removeClass('rad-nav-min');
        }
    });


</script>

